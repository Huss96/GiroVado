<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hussnain.girovado">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".InsertActivity" />
        <activity android:name=".TransportActivity" />
        <activity android:name=".PhotoActivity" />
        <activity android:name=".GalleryAdapter" />
        <activity android:name=".SaveActivity" />
        <activity
            android:name="com.darsh.multipleimageselect.activities.AlbumSelectActivity"
            android:theme="@style/MultipleImageSelectTheme">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity" />
        <activity android:name=".FoundActivity" />
        <activity android:name=".SelectedLeisure" />
        <activity
            android:name=".trial.ImageActicity"
            android:label="@string/title_activity_image_acticity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Prova.ImagesActivity" />
        <activity android:name=".prova.ImageActivity" />
        <activity android:name=".CommentActivity" />
        <activity android:name=".RankingActivity"></activity>
    </application>

</manifest>